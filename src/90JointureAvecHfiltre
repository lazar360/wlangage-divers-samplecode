sFAC_LIV_DAT	est une chaîne
	Ch_datelivraison=FAENTETE.FEDATEFAC
	SI SansEspace(FAENTETE.FENUMBL)<>"" ALORS
		HFiltre(FALIGNE,FLCLE,FAENTETE.NUMSF+hValMin,FAENTETE.NUMSF+hValMax)
		POUR TOUT FALIGNE SUR FLCLE
			SI SansEspace(FALIGNE.FLNUMBL)<>"" ALORS
				SI PG_RECHERCHE_LIENTETE(FALIGNE.FLNUMBL,FALIGNE.FLNBLIV) ALORS
					Ch_datelivraison = SansEspace(LIENTETE.LIDATELIV)
				FIN
				SORTIR
			FIN
		FIN
	FIN
	HRAZ(LIENTETE)
	sFAC_LIV_DAT = Ch_datelivraison

/////////////////////OU//////////////////////////
sFAC_ACPT_DAT	est une chaîne   // BLENTETE.BEACOMPTE,REGL,LIENTETE.LIDATELIV
	HLitRecherchePremier(BLENTETE,NUMBL,FAENTETE.FENUMBL)
	SI BLENTETE.BEACOMPTE <> 0 ALORS
		HLitPremier(LIENTETE, ...
		HFiltre(LIENTETE, "NUMCDE = '" + SansEspace(ExtraitChaîne(FAENTETE.CLE_BL,2,",")) +"'"))
		sFAC_ACPT_DAT = DateVersChaîne(SansEspace(LIENTETE.LIDATELIV),"AAAA-MM-JJ") // A TESTER
		HDésactiveFiltre(LIENTETE)
	FIN
